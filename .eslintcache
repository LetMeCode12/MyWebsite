mywebsite\\src\\components\\sliderImage\\sliderImage.js",["30"],"import React, { useRef } from 'react';\r\nimport \"./sliderImage.scss\"\r\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\r\nimport { faArrowLeft,faArrowRight } from \"@fortawesome/free-solid-svg-icons\";\r\n\r\nfunction SliderImage({ imagesPath }) {\r\n\r\n    const mySlider = useRef();\r\n\r\n    let cnt = 0;\r\n    const leng = imagesPath.length;\r\n\r\n    const moveImages= () => {\r\n        if (mySlider && mySlider.current) {\r\n            mySlider.current.style.left = `-${cnt}00vw`;\r\n            cnt++;\r\n        }\r\n        if (cnt >= leng) {\r\n            cnt = 0;\r\n        }\r\n    }\r\n\r\n    let changeSlide = setInterval(moveImages, 5000)\r\n\r\n    const onNextImage = () => {\r\n        clearInterval(changeSlide)\r\n        cnt++;\r\n        if(cnt>=leng){\r\n            cnt=0;\r\n        }\r\n        mySlider.current.style.left = `-${cnt}00vw`;\r\n        changeSlide = setInterval(moveImages, 5000)\r\n    }\r\n\r\n    const onPrevImage = () => {\r\n        clearInterval(changeSlide)\r\n        cnt--;\r\n        if(cnt<0){\r\n            cnt=leng-1;\r\n        }\r\n        mySlider.current.style.left = `-${cnt}00vw`;\r\n        changeSlide = setInterval(moveImages, 5000)    \r\n    }\r\n\r\n    React.useEffect(() => {\r\n        console.log(\"Images:\", imagesPath)\r\n\r\n\r\n    })\r\n\r\n    const renderImage = (e, index) => {\r\n        return <img className={`myImage ${index}`} src={e} />\r\n    }\r\n\r\n    return (\r\n        <div className=\"sliderImage\" >\r\n                <div className=\"prev\" onClick={onPrevImage}>\r\n                    <FontAwesomeIcon icon={faArrowLeft} size={\"2x\"} />\r\n                </div>\r\n            <div className=\"innerSlider\" ref={mySlider}>\r\n               \r\n                {\r\n                    imagesPath.map((imagePath, index) => renderImage(imagePath, index))\r\n                }\r\n                \r\n            </div>\r\n                <div className=\"next\" onClick={onNextImage}>\r\n                    <FontAwesomeIcon icon={faArrowRight} size={\"2x\"} />\r\n                </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default SliderImage;\r\n","C:\\Users\\Norbert\\Desktop\\MyWebsite\\mywebsite\\src\\components\\navBar\\myNavBar.js",[],"C:\\Users\\Norbert\\Desktop\\MyWebsite\\mywebsite\\src\\components\\footer\\footer.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":52,"column":16,"nodeType":"37","endLine":52,"endColumn":62},"no-native-reassign",["38"],"no-negated-in-lhs",["39"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]